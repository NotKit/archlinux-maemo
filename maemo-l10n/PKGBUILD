package() {
	cd "$pkgdir"
	for i in "${source[@]}"
    do
        filename="$(basename $i)"
        bsdtar -xf "$srcdir/$filename"
        tar xfz data.tar.gz
        rm control.tar.gz data.tar.gz debian-binary
    done
}
